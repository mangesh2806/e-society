<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table-filter match-catalog="e_society" match-name="employee" />
	<table-filter match-catalog="e_society" match-name="member" />
	<table-filter match-name="user_type" match-catalog="e_society"></table-filter>
	<table-filter match-catalog="e_society" match-name="user" />

	<table-filter match-name="flatdetails" match-catalog="e_society"></table-filter>
	<table catalog="e_society" name="user_type">
		<column name="user_type_id"></column>
		<column name="user_type"></column>
	</table>
</hibernate-reverse-engineering>